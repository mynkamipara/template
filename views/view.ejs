<div class="container">

  <table class="table">
    <thead>
      <tr>
        <th scope="col">Index</th>
        <th scope="col">Full_name</th>
        <th scope="col">Email</th>
        <th scope="col">Contact</th>
        <th scope="col">Birthdate</th>
        <th scope="col">Address</th>
        <th scope="col">Operation</th>
        
      </tr>
    </thead>
    <% if (data) { %>
      <% data.forEach(function(user){ %>
        
    <tbody>
      <tr>
        <th scope="row">1</th>
        <td><%= user.fname %></td>
        <td><%= user.email %></td>
        <td><%= user.contact %></td>
        <td><%= user.bod %></td>
        <td><%= user.address %></td>
        <td><button type="button"><a href='./delete/<%=user.keyid%>' onClick="return confirm('Are you sure you want to delete?')">delete</a></button></td>
        <td><button type="button"><a href='./update/<%=user.keyid%>'>Update</a></button></td>
      </tr>
    
     
    </tbody>

    <% }) %>
<% } %>
  </table>

</div>





